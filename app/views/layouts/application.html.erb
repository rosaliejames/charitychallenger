<!DOCTYPE html>
<html>
<head>
  <title>CharityChallenger</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>

<body>
<% if user_signed_in? %>
<%= render partial: "nav_signed_in" %>
<% else %>
<%= render partial: "nav_not_signed_in" %>
<% end %>


<%= yield %>

</body>
</html>
